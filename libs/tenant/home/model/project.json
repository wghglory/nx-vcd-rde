        "jestConfig": "libs/tenant/home/model/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": ["scope:tenant", "domain:home", "type:model"]
}
