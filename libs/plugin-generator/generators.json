{
  "$schema": "http://json-schema.org/schema",
  "name": "plugin-generator",
  "version": "0.0.1",
  "generators": {
    "angular-library": {
      "factory": "./src/generators/angular-library/generator",
      "schema": "./src/generators/angular-library/schema.json",
      "description": "Create an Angular library (data-access,feature,ui)"
    },
    "angular-library-advanced": {
      "factory": "./src/generators/angular-library-advanced/generator",
      "schema": "./src/generators/angular-library-advanced/schema.json",
      "description": "Create an empty Angular library(name,scope,type,optionalDomain)"
    },
    "typescript-library": {
      "factory": "./src/generators/typescript-library/generator",
      "schema": "./src/generators/typescript-library/schema.json",
      "description": "Typescript library for model and util"
    },
    "all-libraries": {
      "factory": "./src/generators/all-libraries/generator",
      "schema": "./src/generators/all-libraries/schema.json",
      "description": "Generate a feature"
    },
    "mfe-app": {
      "factory": "./src/generators/mfe-app/generator",
      "schema": "./src/generators/mfe-app/schema.json",
      "description": "Generate a remote app"
    },
    "mfe-lib": {
      "factory": "./src/generators/mfe-lib/generator",
      "schema": "./src/generators/mfe-lib/schema.json",
      "description": "Generate data-access, feature libs for a remote app"
    },
    "ng-lib": {
      "factory": "./src/generators/ng-lib/generator",
      "schema": "./src/generators/ng-lib/schema.json",
      "description": "Generate Angular Libraries"
    },
    "all-lib": {
      "factory": "./src/generators/all-lib/generator",
      "schema": "./src/generators/all-lib/schema.json",
      "description": "all-lib generator"
    },
    "ts-lib": {
      "factory": "./src/generators/ts-lib/generator",
      "schema": "./src/generators/ts-lib/schema.json",
      "description": "ts-lib generator"
    }
  }
}
