{
  "$schema": "http://json-schema.org/schema",
  "name": "plugin-generator",
  "version": "0.0.1",
  "generators": {
    "plugin-generator": {
      "factory": "./src/generators/plugin-generator/generator",
      "schema": "./src/generators/plugin-generator/schema.json",
      "description": "plugin-generator generator"
    },
    "angular-library": {
      "factory": "./src/generators/angular-library/generator",
      "schema": "./src/generators/angular-library/schema.json",
      "description": "angular-library generator"
    },
    "plugin-mfe": {
      "factory": "./src/generators/plugin-mfe/generator",
      "schema": "./src/generators/plugin-mfe/schema.json",
      "description": "Generate a full CCS UI plugin!"
    },
    "remove": {
      "factory": "./src/generators/remove/generator",
      "schema": "./src/generators/remove/schema.json",
      "description": "Remove a full CCS UI plugin"
    }
  }
}
