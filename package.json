{
  "name": "seed",
  "version": "0.0.1",
  "config": {
    "vip_version": "0.0.1"
  },
  "license": "MIT",
  "scripts": {
    "api": "npx nx serve api",
    "dev:standalone": "NX_BUILD_DATE=$(date) NX_MODE=standalone npx nx serve seed",
    "dev": "NX_BUILD_DATE=$(date) npx nx serve seed",
    "build": "NX_BUILD_DATE=$(date) npx nx build seed",
    "start": "npm run api & npm run dev",
    "e2e": "npx nx run seed-e2e:e2e --headed=false --watch",
    "test": "npx nx run-many --all --target=test --parallel --coverage",
    "_test:ut": "jest --passWithNoTests --coverage --maxWorkers=75% --silent --noStackTrace --watchAll=false --logHeapUsage",
    "_test:ut.ci": "npm run test:ut -- --testSequencer ./tools/parallel_ci_sequencer.js",
    "test:e2e": "CYPRESS_EXCLUDE_TAGS=PROD_ONLY nx e2e seed-e2e",
    "test:e2e.headless": "npm run test:e2e -- --quiet",
    "test:e2e.watch": "npm run test:e2e -- --watch",
    "test:e2e.ci": "run-p \"test:e2e.headless -- --config watchForFileChanges=false --spec {1}\" --",
    "coverage:ut:combine": "node ./tools/merge_coverage_jest.js",
    "coverage:e2e:combine": "node ./tools/merge_coverage_cypress.js",
    "coverage:combine": "./tools/combine_test_reports.sh",
    "coverage:reset": "rm -rf reports",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "lint-staged": "lint-staged --no-stash",
    "prettier": "prettier --config ./.prettierrc  --write --list-different '{apps,libs}/**/*{.ts,.js,.json,.css,.scss,.md,.html}'",
    "fix": "nx run-many --all --target=lint -- --fix",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "update:check": "ng update",
    "dep-graph": "nx dep-graph",
    "_postinstall": "ngcc; mkdir -p tmp; nx dep-graph --file=tmp/output.json",
    "axe": "npx axe --axe-source ./node_modules/axe-core/axe.min.js --verbose --browser chrome",
    "axe.save": "npx axe --dir ./vpat/axe-results",
    "lighthouse": "npx lighthouse --view",
    "lighthouse.save": "npx lighthouse --output json --output html --output-path=./vpat/lighthouse-results/report.json",
    "vip:collect-source": "npm run collect-source-bundle && npm run collect-mixin-bundle",
    "_vip:collect-source-bundle": "collect-source-bundle --source-dir `pwd`/src/app --product seed --component AngularClient --host https://g11n-vip-stg-1.eng.vmware.com:8090 --version $npm_package_config_vip_version",
    "_vip:collect-mixin-bundle": "collect-mixin-source --source-dir `pwd`/src/app --product seed --component AngularClient --host https://g11n-vip-stg-1.eng.vmware.com:8090 --version $npm_package_config_vip_version",
    "vip:compile-mixin": "./tools/compile-mixin.sh",
    "vip:collect-source-bundle": "for dir in ./libs ./apps; do collect-source-bundle --source-dir $dir --product vOSE --component AngularClient --host https://g11n-vip-stg-1.eng.vmware.com:8090 --version $npm_package_config_vip_version -r ${VIP_TOKEN}; done",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{css,scss,component.html}": "prettier --write",
    "*.format:write": "prettier --write"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~15.1.0",
    "@angular/cdk": "^15.1.1",
    "@angular/common": "~15.1.0",
    "@angular/compiler": "~15.1.0",
    "@angular/core": "~15.1.0",
    "@angular/forms": "~15.1.0",
    "@angular/platform-browser": "~15.1.0",
    "@angular/platform-browser-dynamic": "~15.1.0",
    "@angular/router": "~15.1.0",
    "@cds/core": "^6.3.0",
    "@clr/angular": "^15.0.0-rc.1",
    "@clr/ui": "^15.0.0-rc.1",
    "@ngrx/component-store": "~15.1.0",
    "@ngrx/effects": "~15.1.0",
    "@ngrx/entity": "~15.1.0",
    "@ngrx/router-store": "~15.1.0",
    "@ngrx/store": "~15.1.0",
    "@nrwl/angular": "15.6.1",
    "@swc/helpers": "~0.4.11",
    "@types/lodash": "^4.14.191",
    "@vmw/ngx-utils": "^14.6.0",
    "@vmw/ngx-vip": "^14.1.1",
    "axios": "^1.0.0",
    "dompurify": "^2.4.3",
    "express": "^4.18.1",
    "lodash": "^4.17.21",
    "ngx-infinite-scroll": "^15.0.0",
    "ngx-pipes": "^3.2.0",
    "parse-url": "^8.1.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~15.1.0",
    "@angular-devkit/core": "~15.1.0",
    "@angular-devkit/schematics": "^15.1.4",
    "@angular-eslint/eslint-plugin": "~15.0.0",
    "@angular-eslint/eslint-plugin-template": "~15.0.0",
    "@angular-eslint/template-parser": "~15.0.0",
    "@angular/cli": "~15.1.0",
    "@angular/compiler-cli": "~15.1.0",
    "@angular/language-service": "~15.1.0",
    "@faker-js/faker": "^7.6.0",
    "@hirez_io/observer-spy": "^2.2.0",
    "@ngrx/schematics": "~15.1.0",
    "@ngrx/store-devtools": "~15.1.0",
    "@nrwl/cypress": "15.6.1",
    "@nrwl/devkit": "15.6.3",
    "@nrwl/eslint-plugin-nx": "15.6.1",
    "@nrwl/express": "15.6.1",
    "@nrwl/jest": "^15.6.3",
    "@nrwl/js": "15.6.3",
    "@nrwl/linter": "15.6.1",
    "@nrwl/node": "15.6.1",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/nx-plugin": "^15.6.1",
    "@nrwl/webpack": "15.6.1",
    "@nrwl/workspace": "15.6.1",
    "@rsql/ast": "^1.5.2",
    "@rsql/parser": "^1.5.2",
    "@swc-node/register": "^1.4.2",
    "@swc/cli": "~0.1.55",
    "@swc/core": "^1.2.173",
    "@testing-library/angular": "^13.1.0",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.1",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "autoprefixer": "^10.4.0",
    "axe-core": "^4.6.3",
    "cors": "^2.8.5",
    "cypress": "^12.2.0",
    "cypress-axe": "^1.3.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^9.0.0",
    "husky": "^7.0.4",
    "hygen": "^6.2.11",
    "jasmine-marbles": "~0.9.1",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "jest-preset-angular": "~12.2.3",
    "jsonc-eslint-parser": "^2.1.0",
    "ng-mocks": "^14.6.0",
    "nx": "15.6.1",
    "postcss": "^8.4.5",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "tailwindcss": "^3.0.2",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.8.2"
  }
}
