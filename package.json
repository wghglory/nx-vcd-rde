{
  "name": "seed",
  "version": "0.0.1",
  "config": {
    "vip_version": "0.0.1"
  },
  "license": "MIT",
  "scripts": {
    "api": "npx nx serve api",
    "dev:standalone": "NX_BUILD_DATE=$(date) NX_MODE=standalone npx nx serve seed",
    "dev.host": "NX_BUILD_DATE=$(date) npx nx serve seed",
    "dev": "npm run dev.host -- --open --devRemotes=harbor-mfe",
    "build": "NX_BUILD_DATE=$(date) npx nx build seed",
    "start": "npm run api & npm run dev",
    "e2e": "npx nx run seed-e2e:e2e",
    "e2e:watch": "npm run e2e -- --watch",
    "test": "npx nx run-many --all --target=test --parallel=6 --runInBand",
    "test:coverage": "npm test -- --coverage",
    "coverage:ut": "node ./tools/report-ut-coverage.js && open -a 'Google Chrome' coverage/report/index.html",
    "_coverage:e2e:combine": "node ./tools/merge_coverage_cypress.js",
    "coverage:combine": "./tools/combine_test_reports.sh",
    "precoverage:combine": "rm -rf reports",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "lint-staged": "lint-staged --no-stash",
    "prettier": "prettier --config ./.prettierrc  --write --list-different '{apps,libs}/**/*{.ts,.js,.json,.css,.scss,.md,.html}'",
    "fix": "nx run-many --all --target=lint -- --fix",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "dep-graph": "nx dep-graph",
    "_postinstall": "ngcc; mkdir -p tmp; nx dep-graph --file=tmp/output.json",
    "axe": "npx axe --verbose --browser chrome",
    "axe.save": "npx axe --dir ./vpat/axe-results",
    "lighthouse": "npx lighthouse --view",
    "lighthouse.save": "npx lighthouse --output json --output html --output-path=./vpat/lighthouse-results/lighthouse.json",
    "vip:collect-source": "npm run collect-source-bundle && npm run collect-mixin-bundle",
    "_vip:collect-source-bundle": "collect-source-bundle --source-dir `pwd`/src/app --product seed --component AngularClient --host https://g11n-vip-stg-1.eng.vmware.com:8090 --version $npm_package_config_vip_version",
    "_vip:collect-mixin-bundle": "collect-mixin-source --source-dir `pwd`/src/app --product seed --component AngularClient --host https://g11n-vip-stg-1.eng.vmware.com:8090 --version $npm_package_config_vip_version",
    "vip:compile-mixin": "./tools/compile-mixin.sh",
    "vip:collect-source-bundle": "for dir in ./libs ./apps; do collect-source-bundle --source-dir $dir --product vOSE --component AngularClient --host https://g11n-vip-stg-1.eng.vmware.com:8090 --version $npm_package_config_vip_version -r ${VIP_TOKEN}; done",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{css,scss,component.html}": "prettier --write",
    "*.format:write": "prettier --write"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "18.2.0",
    "@angular/cdk": "18.2.0",
    "@angular/common": "18.2.0",
    "@angular/compiler": "18.2.0",
    "@angular/core": "18.2.0",
    "@angular/forms": "18.2.0",
    "@angular/platform-browser": "18.2.0",
    "@angular/platform-browser-dynamic": "18.2.0",
    "@angular/router": "18.2.0",
    "@cds/core": "^6.13.0",
    "@clr/angular": "^17.2.1",
    "@clr/ui": "^17.2.1",
    "@ngrx/component-store": "18.0.2",
    "@ngrx/effects": "18.0.2",
    "@ngrx/entity": "18.0.2",
    "@ngrx/router-store": "18.0.2",
    "@ngrx/store": "18.0.2",
    "@nx/angular": "19.6.0",
    "@swc/helpers": "0.5.12",
    "@types/lodash": "^4.14.191",
    "@vmw/ngx-utils": "^17.1.0",
    "@vmw/ngx-vip": "^17.0.1",
    "axios": "1.7.4",
    "dompurify": "^2.4.3",
    "express": "^4.18.1",
    "lodash": "^4.17.21",
    "ngx-infinite-scroll": "^18.0.0",
    "ngx-pipes": "^3.2.2",
    "parse-url": "^8.1.0",
    "rxjs": "7.8.1",
    "tslib": "^2.3.0",
    "zone.js": "0.14.10"
  },
